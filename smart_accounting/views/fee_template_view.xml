<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id='fee_template_from_view' model='ir.ui.view'>
		<field name='name'>fee.template.form</field>
		<field name='model'>fee.template</field>
		<field name='arch' type='xml'>
            <form string="Fee Template">
                <sheet>
                    <group>
                        <group>
                            <field name='name'/>
                        </group>
                    </group>
                </sheet>
            </form>
		</field>
	</record>

    <record id='fee_template_tree_view' model='ir.ui.view'>
		<field name='name'>fee.template.form</field>
		<field name='model'>fee.template</field>
		<field name='arch' type='xml'>
            <tree string="Fee Template">
                <field name='name'/>
            </tree>
        </field>
    </record>

    <record id="fee_template_action" model="ir.actions.act_window">
        <field name="name">Fee Template</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">fee.template</field>
        <field name="view_mode">form</field>
        <field name="target">inline</field>
    </record>

    <menuitem id="fee_template_main_menu"
        name="Fee Template"
              sequence="0"/>

    <menuitem id="fee_template_menu"
        name="Fee Template"
        parent="fee_template_main_menu"
        sequence="0"
        action="fee_template_action"
        />



</odoo>